{
  "name": "khs-toy-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:db": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/database/ormconfig.ts",
    "typeorm:db:show": "npm run typeorm:db migration:show",
    "typeorm:db:run": "npm run typeorm:db migration:run",
    "typeorm:db:revert": "npm run typeorm:db migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^7.11.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.18",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/sequelize": "^0.2.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^7.2.0",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server": "^2.25.2",
    "apollo-server-express": "^2.25.2",
    "class-validator": "^0.13.1",
    "dayjs": "^1.10.5",
    "graphql": "^15.5.1",
    "graphql-query-complexity": "^0.8.1",
    "graphql-redis-subscriptions": "^2.4.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-tools": "^7.0.5",
    "pg": "^8.7.3",
    "psql": "0.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "class-transformer": "^0.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@types/passport-jwt": "^3.0.5",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "prettier": "^2.3.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
